@model IEnumerable<MVC.Areas.AdminPanel.Models.VMs.Report.GetProductAccordingToLikeVM>

@{
    ViewData["Title"] = "GetProductAccordingToLike";
}

<h1>Get Product According To Like</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ProductName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Likes)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.ProductName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Likes)
                </td>
            </tr>
        }
    </tbody>
</table>
