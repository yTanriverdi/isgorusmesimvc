@model IEnumerable<MVC.Areas.AdminPanel.Models.VMs.Report.GetProductAccordingToViewsVM>

@{
    ViewData["Title"] = "GetProductAccordingToViews";
}

<h1>Get Product According To Views</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ProductName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Views)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.ProductName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Views)
                </td>
            </tr>
        }
    </tbody>
</table>
